{
  "version": 2,
  "headers": [
    {
      "source": "/assets/(.*\\.(otf|ttf|woff|woff2))",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/opentype"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*\\.(svg|png|jpg|jpeg))",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

